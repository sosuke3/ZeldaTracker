<Window x:Class="ZeldaTracker.MapsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZeldaTracker"
        mc:Ignorable="d"
        Title="MapsWindow" Height="300" Width="558.334">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0">
            <Image Name="LightWorldMap" Source="images/lightworld.png"></Image>
        </Grid>
        <Grid Grid.Column="1">
            <Image Name="DarkWorldMap" Source="images/darkworld.png"></Image>
        </Grid>
    </Grid>
</Window>
